USE GRAPH social
DROP JOB  load_social

CREATE LOADING JOB load_social FOR GRAPH social {
    DEFINE FILENAME vfile;
    DEFINE FILENAME dir_efile;
    DEFINE FILENAME und_efile;
    DEFINE FILENAME mixed_efile;

    LOAD vfile
        TO VERTEX Person VALUES ($0, $0, $1, $2, _)
        USING header="true";
    LOAD dir_efile
        TO VERTEX Person VALUES ($0, $0, _, _, _),
        TO VERTEX Person VALUES ($1, $1, _, _, _),
        TO EDGE Friend VALUES ($0, $1, $2, $3, _)
        USING header="true";
    LOAD und_efile
        TO VERTEX Person VALUES ($0, $0, _, _, _),
        TO VERTEX Person VALUES ($1, $1, _, _, _),
        TO EDGE Coworker VALUES ($0, $1, $2, $3, _)
        USING header="true";
    LOAD mixed_efile
        TO VERTEX Person VALUES ($0, $0, _, _, _),
        TO VERTEX Person VALUES ($1, $1, _, _, _),
        TO EDGE Friend VALUES ($0, $1, $3, $4, _),
        TO EDGE Coworker VALUES ($0, $2, $3, $4, _)
        USING header="true";
}
#RUN LOADING JOB load_social USING dir_efile="./data/data.csv"
